{
    "name": "kuribon",
    "displayName": "Kuribon",
    "description": "lua help tools",
    "version": "0.2.0",
    "publisher": "uc10d",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.addHeadNote",
        "onCommand:extension.formatEntity"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.addHeadNote",
                "title": "head note",
                "key": "shift+cmd+1"
            },
            {
                "command": "extension.formatEntity",
                "title": "init and creat entity",
                "key": "shift+cmd+2"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
